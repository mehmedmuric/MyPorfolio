generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Testimonial {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  name    String
  role    String?
  comment String
  image   String?
  createdAt DateTime @default(now())
  ip        String? 
}